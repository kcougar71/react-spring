{
  "include": ["*.ts", "useChain.tsx"],
  "compilerOptions": {
    "target": "es2017",
    "moduleResolution": "node",
    "lib": ["dom", "es2017"],
    "noEmit": true,
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true
  }
}
